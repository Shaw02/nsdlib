000000r 1               ;=======================================================================
000000r 1               ;	void    __fastcall__    nsd_main(void );
000000r 1               ;-----------------------------------------------------------------------
000000r 1               ;<<Contents>>
000000r 1               ;	main process of BGM and SE
000000r 1               ;<<Input>>
000000r 1               ;	nothing
000000r 1               ;<<Output>>
000000r 1               ;	nothing
000000r 1               ;=======================================================================
000000r 1               .proc	_nsd_main: near
000000r 1               
000000r 1  A9 01        	lda	#nsd_flag::BGM
000002r 1  24 rr        	bit	__flag
000004r 1  D0 46        	bne	BGM_Exit		;BGM disable ?
000006r 1               
000006r 1               	;-------------------------------
000006r 1               	;Tempo
000006r 1  A5 rr        	lda	__Tempo_ctr
000008r 1  18 65 rr     	add	__Tempo
00000Br 1  85 rr        	sta	__Tempo_ctr
00000Dr 1  90 24        	bcc	BGM_SEQ_Exit
00000Fr 1               
00000Fr 1               	;-------------------------------
00000Fr 1               	;BGM
00000Fr 1               BGM_Begin:
00000Fr 1  A2 00 20 rr  	.repeat	nsd::BGM_Track, I
000013r 1  rr A2 02 20  
000017r 1  rr rr A2 04  
00001Br 1  20 rr rr A2  
00001Fr 1  06 20 rr rr  
000023r 1  A2 08 20 rr  
000027r 1  rr           
000028r 1               		ldx	#I*2
000028r 1               		jsr	nsd_sequence
000028r 1               	.endrepeat
000028r 1               
000028r 1  A5 rr        	lda	__Tempo_ctr
00002Ar 1  38 E9 96     	sub	#150
00002Dr 1  85 rr        	sta	__Tempo_ctr
00002Fr 1  C9 6A        	cmp	#106
000031r 1  90 DC        	bcc	BGM_Begin
