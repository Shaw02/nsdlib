000000r 2               		sbc     #>(.right (.tcount ({arg})-1, {arg}))
000000r 2               	.else
000000r 2               		; assume absolute or zero page
000000r 2               		sbc     arg
000000r 2               		sbc     1+(arg)
000000r 2               	.endif
000000r 2               .endmacro
