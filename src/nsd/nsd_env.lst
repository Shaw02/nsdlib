000000r 1               ;=======================================================================
000000r 1               ;	void	nsd_sequence(void);
000000r 1               ;-----------------------------------------------------------------------
000000r 1               ;<<Contents>>
000000r 1               ;
000000r 1               ;<<Input>>
000000r 1               ;	x	Channel * 2
000000r 1               ;<<Output>>
000000r 1               ;	nothing
000000r 1               ;=======================================================================
000000r 1               .proc	nsd_envelop
000000r 1               
000000r 1  86 rr        	stx	__channel
000002r 1               
000002r 1               	;-------------------------------
000002r 1               	;Frequency (note)
000002r 1               Frequency:
000002r 1  BD rr rr     	lda	__chflag,x
000005r 1  29 03        	and	#nsd_chflag::KeyOff
000007r 1  D0 05        	bne	@L
000009r 1  A9 00        	lda	#0			; if (rest with vol=0){
00000Br 1  4C rr rr     	jmp	_nsd_snd_volume		;	volume = 0
00000Er 1               @L:					; } else {
00000Er 1  BD rr rr     	lda	__note,x
000011r 1  18 7D rr rr  	add	__trans,x
000015r 1  85 rr        	sta	__tmp + 1
000017r 1  0A 0A 0A 0A  	shl	a, 4
00001Br 1  85 rr        	sta	__tmp
00001Dr 1  A5 rr        	lda	__tmp + 1
00001Fr 1  4A 4A 4A 4A  	shr	a, 4
000023r 1  85 rr        	sta	__tmp + 1		;__tmp = (int)(__note << 4)
000025r 1  4C rr rr     	jmp	debug
000028r 1               
000028r 1               	;-----------
000028r 1               	;Detune
000028r 1  BD rr rr     Detune:	lda	__detune_cent,x
00002Br 1  30 05        	bmi	@L
00002Dr 1  A0 00        	ldy	#$00
00002Fr 1  4C rr rr     	jmp	@E
000032r 1  A0 FF        @L:	ldy	#$FF			; ay = __detune_cent (sign expand)
000034r 1  18 65 rr     @E:	add	__tmp
000037r 1  85 rr        	sta	__tmp
000039r 1  98           	tya
00003Ar 1  65 rr        	adc	__tmp + 1
00003Cr 1  85 rr        	sta	__tmp + 1		;__tmp += (signed int)__detune_cent
00003Er 1               
00003Er 1               	;-----------
00003Er 1               	;Envelop Frequency
00003Er 1  BD rr rr     F_Env:	lda	__env_frequency + 1,x
000041r 1  F0 0E        	beq	@Freq_Exit
000043r 1  85 rr        	sta	__ptr + 1
000045r 1  BD rr rr     	lda	__env_frequency,x
000048r 1  85 rr        	sta	__ptr
00004Ar 1  BC rr rr     	ldy	__env_freq_ptr,x
00004Dr 1               	;
00004Dr 1               	;¡¡	to do envelop process
00004Dr 1               	;
00004Dr 1  98           	tya
00004Er 1  9D rr rr     	sta	__env_freq_ptr,x
000051r 1               
000051r 1               @Freq_Exit:
000051r 1               
000051r 1               	;-----------
000051r 1               	;Envelop Note
000051r 1  BD rr rr     	lda	__env_note + 1,x
000054r 1  F0 0E        	beq	@Note_Exit
000056r 1  85 rr        	sta	__ptr + 1
000058r 1  BD rr rr     	lda	__env_note,x
00005Br 1  85 rr        	sta	__ptr
00005Dr 1  BC rr rr     	ldy	__env_note_ptr,x
000060r 1               	;
000060r 1               	;¡¡	to do envelop process
000060r 1               	;
000060r 1  98           	tya
000061r 1  9D rr rr     	sta	__env_note_ptr,x
000064r 1               
000064r 1               @Note_Exit:
000064r 1               
000064r 1               	;-----------
000064r 1               	;Setting device (APU)
000064r 1               debug:
000064r 1  A5 rr        	lda	__tmp
000066r 1  A6 rr        	ldx	__tmp + 1
000068r 1  20 rr rr     	jsr	_nsd_snd_frequency	;nsd_snd_frequency(ax);
00006Br 1  A6 rr        	ldx	__channel
00006Dr 1               
00006Dr 1               	;-------------------------------
00006Dr 1               	;Voice & Volume
00006Dr 1  E0 04        	cpx	#nsd::TR_BGM3
00006Fr 1  F0 5E        	beq	exit
000071r 1  E0 08        	cpx	#nsd::TR_BGM5
000073r 1  F0 5A        	beq	exit
000075r 1               
000075r 1               	;-------------------------------
000075r 1               	;Voice
000075r 1               Voice:
000075r 1  BD rr rr     	lda	__chflag,x
000078r 1  29 02        	and	#$02
00007Ar 1  D0 0A        	bne	@Envelop
00007Cr 1  BD rr rr     	lda	__voice,x
00007Fr 1  4A 4A 4A 4A  	shr	a, 4			; a = release voice
000083r 1  4C rr rr     	jmp	Set_Voice
000086r 1               @Envelop:
000086r 1  BD rr rr     	lda	__env_voice + 1,x
000089r 1  D0 04        	bne	@L
00008Br 1  A9 02        	lda	#$02
