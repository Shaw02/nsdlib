000040r 1               ;	void	__fastcall__	_nsd_mul(unsigned char a, unsigned char x);
000040r 1               ;-----------------------------------------------------------------------
000040r 1               ;<<Contents>>
000040r 1               ;	multiplication	8bit = 4bit * 4bit
000040r 1               ;<<Input>>
000040r 1               ;	a	value 1 (lower 4bit)	volume side
000040r 1               ;	x	value 2 (lower 4bit)	envelop
000040r 1               ;<<Output>>
000040r 1               ;	a =	a × (x ＋ 1)
000040r 1               ;=======================================================================
000040r 1               .proc	_nsd_mul
000040r 1               .code
000040r 1               
000040r 1  29 0F        	and	#$0F
000042r 1  0A 0A 0A     	shl	a, 3
000045r 1  85 rr        	sta	__tmp + 1		; = volume << 3
000047r 1               
000047r 1  E8           	inx
000048r 1  8A           	txa
000049r 1  49 FF        	eor	#$FF
00004Br 1  85 rr        	sta	__tmp			; = ^ (envelop+1)
00004Dr 1               
00004Dr 1  A9 00        	lda	#0
00004Fr 1  46 rr        	lsr	__tmp
000051r 1  B0 02        	bcs	@L1
000053r 1  65 rr        	adc	__tmp + 1
000055r 1               @L1:
000055r 1  4A           	lsr	a
000056r 1  46 rr        	lsr	__tmp
000058r 1  B0 02        	bcs	@L2
00005Ar 1  65 rr        	adc	__tmp + 1
00005Cr 1               @L2:
00005Cr 1  4A           	lsr	a
00005Dr 1  46 rr        	lsr	__tmp
00005Fr 1  B0 02        	bcs	@L3
000061r 1  65 rr        	adc	__tmp + 1
000063r 1               @L3:
000063r 1  4A           	lsr	a
000064r 1  46 rr        	lsr	__tmp
000066r 1  B0 02        	bcs	@L4
000068r 1  65 rr        	adc	__tmp + 1
00006Ar 1               @L4:
00006Ar 1  4A           	lsr	a
00006Br 1  46 rr        	lsr	__tmp
00006Dr 1  B0 02        	bcs	@L5
00006Fr 1  65 rr        	adc	__tmp + 1
000071r 1               @L5:
000071r 1               exit:
000071r 1  0A           	shl	a,1
000072r 1  60           	rts
000073r 1               .endproc
