000040r 1               ;=======================================================================
000040r 1               ;	void	__fastcall__	_nsd_mul(unsigned char a, unsigned char x);
000040r 1               ;-----------------------------------------------------------------------
000040r 1               ;<<Contents>>
000040r 1               ;	multiplication	8bit = 4bit * 4bit
000040r 1               ;<<Input>>
000040r 1               ;	a	value 1 (lower 4bit)	volume side
000040r 1               ;	x	value 2 (lower 4bit)	envelop
