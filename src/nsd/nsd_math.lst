000000r 1               ;=======================================================================
000000r 1               ;	void	__fastcall__	_nsd_div192(unsigned int ax);
000000r 1               ;-----------------------------------------------------------------------
000000r 1               ;<<Contents>>
000000r 1               ;	Divide `ax' by 192 (0xC0) = 16 * 12
000000r 1               ;<<Input>>
000000r 1               ;	ax	value
000000r 1               ;<<Output>>
000000r 1               ;	a =	ax mod 192
000000r 1               ;	x =	ax  /  192
000000r 1               ;=======================================================================
000000r 1               .proc	_nsd_div192
000000r 1               .code
000000r 1  48           	pha		; save a
000001r 1  8A           	txa
000002r 1  A8           	tay		; y = x
000003r 1  A9 00        	lda	#0
000005r 1  AA           	tax		; x = 0
000006r 1  68           	pla		; restore a
000007r 1               
000007r 1               	;if (ax > 0x0600) then
000007r 1  C0 06        	cpy	#$06
000009r 1  90 07        	bcc	Div_8
00000Br 1  88           	dey
00000Cr 1  88           	dey
00000Dr 1  88           	dey	; y -= 3
00000Er 1  E8           	inx
00000Fr 1  E8           	inx
000010r 1  E8           	inx
000011r 1  E8           	inx	; x += 4
000012r 1               
000012r 1               Div_8:	;if (ax > 0x0300) then
000012r 1  C0 03        	cpy	#$03
000014r 1  90 07        	bcc	Div_4
000016r 1  88           	dey
000017r 1  88           	dey
000018r 1  88           	dey	; y -= 3
000019r 1  E8           	inx
00001Ar 1  E8           	inx
00001Br 1  E8           	inx
00001Cr 1  E8           	inx	; x += 4
00001Dr 1               
00001Dr 1               Div_4:	;if (ax > 0x0180) then
00001Dr 1  C0 01        	cpy	#$01
00001Fr 1  90 0F        	bcc	Div_2
000021r 1  D0 04        	bne	@L2
000023r 1  C9 80        	cmp	#$80
000025r 1  90 09        	bcc	Div_2
000027r 1  38 E9 80     @L2:	sub	#$80
00002Ar 1  B0 01        	bcs	@L
00002Cr 1  88           	dey
00002Dr 1  88           @L:	dey
00002Er 1  E8           	inx
00002Fr 1  E8           	inx	; x += 2
000030r 1               
000030r 1               Div_2:	;if (ax > 0x00C0) then
000030r 1  C0 00        	cpy	#$00
000032r 1  D0 04        	bne	@L2
000034r 1  C9 C0        	cmp	#$C0
000036r 1  90 07        	bcc	exit
000038r 1  38 E9 C0     @L2:	sub	#$C0
00003Br 1  B0 01        	bcs	@L
00003Dr 1  88           	dey
00003Er 1  E8           @L:	inx
00003Fr 1               
00003Fr 1               exit:
00003Fr 1  60           	rts
000040r 1               .endproc
